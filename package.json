{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "A modern portfolio website with Spring Boot backend and React frontend",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && ./mvnw spring-boot:run",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "cd frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "portfolio",
    "spring-boot",
    "react",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT"
}